{
  "name": "plc-oven-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:server": "cd plc-server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:docs": "cd docs && npm run dev",
    "dev": "npm run dev:server & npm run dev:frontend",
    "build:server": "cd plc-server && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:docs": "cd docs && npm run build",
    "build": "npm run build:server && npm run build:frontend && npm run build:docs",
    "start:server": "cd plc-server && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "start:docs": "cd docs && npm run start"
  },
  "devDependencies": {
    "vitepress": "^1.0.0"
  }
}
